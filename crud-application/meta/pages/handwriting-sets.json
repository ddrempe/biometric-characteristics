{
  "components": [
    {
      "name": "grid0",
      "allowDelete": true,
      "allowAdd": true,
      "allowFiltering": true,
      "allowMultiSorting": false,
      "allowPaging": true,
      "allowSorting": true,
      "col": 12,
      "count": "${getHandwritingSetsCount}",
      "data": "${getHandwritingSetsResult}",
      "events": {
        "loadData": [
          {
            "name": "biometric characteristics model.getHandwritingSets",
            "parameters": [
              {
                "name": "$filter",
                "value": "${event.filter}"
              },
              {
                "name": "$top",
                "value": "this.grid0.allowPaging ? event.top : null"
              },
              {
                "name": "$skip",
                "value": "this.grid0.allowPaging ? event.skip : null"
              },
              {
                "name": "$orderby",
                "value": "${event.orderby}"
              },
              {
                "name": "$count",
                "value": "this.grid0.allowPaging"
              },
              {
                "name": "$expand",
                "value": "SampleSet,TextSet"
              }
            ],
            "then": [
              {
                "name": "getHandwritingSetsResult",
                "type": "set",
                "value": "${result.value}"
              },
              {
                "name": "getHandwritingSetsCount",
                "type": "set",
                "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
              }
            ],
            "type": "invoke"
          }
        ],
        "delete": [
          {
            "error": [
              {
                "detail": "Unable to delete HandwritingSet",
                "severity": "error",
                "summary": "Error",
                "type": "notify"
              }
            ],
            "name": "biometric characteristics model.deleteHandwritingSet",
            "parameters": [
              {
                "name": "Id",
                "value": "${event.Id}"
              }
            ],
            "then": [
              {
                "detail": "HandwritingSet deleted!",
                "severity": "success",
                "summary": "Success",
                "type": "notify"
              }
            ],
            "type": "invoke"
          }
        ],
        "select": [
          {
            "parameters": [
              {
                "name": "Id",
                "value": "${event.Id}"
              }
            ],
            "path": "edit-handwriting-set",
            "type": "popup"
          }
        ],
        "add": [
          {
            "path": "add-handwriting-set",
            "type": "popup"
          }
        ]
      },
      "row": 0,
      "section": null,
      "title": "",
      "columns": [
        {
          "property": "Id",
          "title": "Id",
          "type": "integer"
        },
        {
          "property": "Sample_Id",
          "template": "${data.SampleSet?.FilePath}",
          "title": "Sample Set",
          "type": "integer"
        },
        {
          "property": "TextId",
          "template": "${data.TextSet?.Content}",
          "title": "Text Set",
          "type": "integer"
        }
      ],
      "type": "grid"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getHandwritingSets",
        "parameters": [
          {
            "name": "$top",
            "value": "this.grid0.allowPaging ? this.grid0.pageSize : null"
          },
          {
            "name": "$skip",
            "value": "this.grid0.allowPaging ? 0 : null"
          },
          {
            "name": "$count",
            "value": "this.grid0.allowPaging"
          },
          {
            "name": "$expand",
            "value": "SampleSet,TextSet"
          }
        ],
        "then": [
          {
            "name": "getHandwritingSetsResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getHandwritingSetsCount",
            "type": "set",
            "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Handwriting Sets"
}