{
  "components": [
    {
      "col": 12,
      "data": "${keyboardset}",
      "events": {
        "cancel": [
          {
            "type": "back"
          }
        ],
        "submit": [
          {
            "error": [
              {
                "detail": "Unable to update KeyboardSet",
                "severity": "error",
                "summary": "Error",
                "type": "notify"
              }
            ],
            "name": "biometric characteristics model.updateKeyboardSet",
            "parameters": [
              {
                "name": "Id",
                "value": "${parameters.Id}"
              },
              {
                "name": "KeyboardSet",
                "value": "${event}"
              }
            ],
            "then": [
              {
                "path": "keyboard-sets",
                "type": "navigate"
              }
            ],
            "type": "invoke"
          }
        ]
      },
      "fields": [
        {
          "property": "Description",
          "required": false,
          "title": "Description",
          "type": "string"
        },
        {
          "property": "Name",
          "required": true,
          "title": "Name",
          "type": "string"
        },
        {
          "property": "Type",
          "required": false,
          "title": "Type",
          "type": "string"
        }
      ],
      "row": 0,
      "section": null,
      "name": "form0",
      "type": "form"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getKeyboardSetById",
        "parameters": [
          {
            "name": "Id",
            "value": "${parameters.Id}"
          }
        ],
        "then": [
          {
            "name": "keyboardset",
            "type": "set",
            "value": "${result}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Keyboard Set"
}