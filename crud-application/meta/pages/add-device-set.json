{
  "components": [
    {
      "col": 12,
      "data": "",
      "events": {
        "cancel": [
          {
            "type": "back"
          }
        ],
        "submit": [
          {
            "error": [
              {
                "detail": "Unable to create new DeviceSet!",
                "severity": "error",
                "summary": "Error",
                "type": "notify"
              }
            ],
            "name": "biometric characteristics model.createDeviceSet",
            "parameters": [
              {
                "name": "DeviceSet",
                "value": "${event}"
              }
            ],
            "then": [
              {
                "path": "device-sets",
                "type": "navigate"
              }
