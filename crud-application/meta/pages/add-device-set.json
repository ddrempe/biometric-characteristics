            "type": "invoke"
          }
        ]
      },
      "fields": [
        {
          "property": "Description",
          "required": false,
          "title": "Description",
          "type": "string"
        },
        {
          "property": "Manufacturer",
          "required": false,
          "title": "Manufacturer",
          "type": "string"
        },
        {
          "property": "Name",
          "required": true,
          "title": "Name",
          "type": "string"
        },
        {
          "data": "${getDeviceTypeSetsResult}",
          "property": "DeviceTypeId",
          "required": false,
          "textProperty": "Name",
          "title": "Device Type Set",
          "type": "lookup",
          "valueProperty": "Id",
          "placeholder": "Choose DeviceTypeSet"
        }
      ],
      "row": 0,
      "section": null,
      "name": "form0",
      "type": "form"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getDeviceTypeSets",
        "parameters": [],
        "then": [
          {
            "name": "getDeviceTypeSetsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Add Device Set"
}