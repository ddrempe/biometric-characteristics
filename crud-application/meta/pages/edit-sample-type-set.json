        {
          "property": "Name",
          "required": true,
          "title": "Name",
          "type": "string"
        }
      ],
      "row": 0,
      "section": null,
      "name": "form0",
      "type": "form"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getSampleTypeSetById",
        "parameters": [
          {
            "name": "Id",
            "value": "${parameters.Id}"
          }
        ],
        "then": [
