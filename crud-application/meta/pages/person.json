{
  "access": "",
  "components": [
    {
      "allowAdd": true,
      "allowDelete": true,
      "allowFiltering": true,
      "allowMultiSorting": false,
      "allowPaging": true,
      "allowSorting": true,
      "col": 12,
      "columns": [
        {
          "property": "Id",
          "title": "Id",
          "type": "integer"
        },
        {
          "format": "date-time",
          "property": "DateOfBirth",
          "template": "${data.DateOfBirth | utcDate}",
          "title": "Date Of Birth",
          "type": "string"
        },
        {
          "property": "FirstName",
          "title": "First Name",
          "type": "string"
        },
        {
          "property": "LastName",
          "title": "Last Name",
          "type": "string"
        },
        {
          "property": "Gender",
          "title": "Gender",
          "type": "string"
        },
        {
          "property": "Address",
          "title": "Address",
          "type": "string"
        }
      ],
      "count": "${getPersonSetsCount}",
      "data": "${getPersonSetsResult}",
      "events": {
        "add": [
          {
            "path": "add-person-set",
            "type": "popup"
          }
        ],
        "delete": [
          {
            "error": [
              {
                "detail": "Unable to delete PersonSet",
                "severity": "error",
                "summary": "Error",
                "type": "notify"
              }
            ],
            "name": "biometric characteristics model.deletePersonSet",
            "parameters": [
              {
                "name": "Id",
                "value": "${event.Id}"
              }
            ],
            "then": [
              {
                "detail": "PersonSet deleted!",
                "severity": "success",
                "summary": "Success",
                "type": "notify"
              }
            ],
            "type": "invoke"
          }
        ],
        "loadData": [
          {
            "name": "biometric characteristics model.getPersonSets",
            "parameters": [
              {
                "name": "$filter",
                "value": "${event.filter}"
              },
              {
                "name": "$top",
                "value": "this.grid0.allowPaging ? event.top : null"
              },
              {
                "name": "$skip",
                "value": "this.grid0.allowPaging ? event.skip : null"
              },
              {
                "name": "$orderby",
                "value": "${event.orderby}"
              },
              {
                "name": "$count",
                "value": "this.grid0.allowPaging"
              },
              {
                "name": "$expand",
                "value": ""
              }
            ],
            "then": [
              {
                "name": "getPersonSetsResult",
                "type": "set",
                "value": "${result.value}"
              },
              {
                "name": "getPersonSetsCount",
                "type": "set",
                "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
              }
            ],
            "type": "invoke"
          }
        ],
        "rowSelect": [
          {
            "parameters": [
              {
                "name": "Id",
                "value": "${event.Id}"
              }
            ],
            "path": "edit-person-set",
            "type": "popup"
          }
        ]
      },
      "name": "grid0",
      "row": 0,
      "section": null,
      "title": "",
      "type": "grid"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getPersonSets",
        "parameters": [
          {
            "name": "$top",
            "value": "this.grid0.allowPaging ? this.grid0.pageSize : null"
          },
          {
            "name": "$skip",
            "value": "this.grid0.allowPaging ? 0 : null"
          },
          {
            "name": "$count",
            "value": "this.grid0.allowPaging"
          }
        ],
        "then": [
          {
            "name": "getPersonSetsResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getPersonSetsCount",
            "type": "set",
            "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Person"
}