{
  "components": [
    {
      "col": 12,
      "data": "${irisset}",
      "events": {
        "cancel": [
          {
            "type": "back"
          }
        ],
        "submit": [
          {
            "error": [
              {
                "detail": "Unable to update IrisSet",
                "severity": "error",
                "summary": "Error",
                "type": "notify"
              }
            ],
            "name": "biometric characteristics model.updateIrisSet",
            "parameters": [
              {
                "name": "Id",
                "value": "${parameters.Id}"
              },
              {
                "name": "IrisSet",
                "value": "${event}"
              }
            ],
            "then": [
              {
                "path": "iris-sets",
                "type": "navigate"
              }
            ],
            "type": "invoke"
          }
        ]
      },
      "fields": [
        {
          "property": "Eye",
          "required": false,
          "title": "Eye",
          "type": "string"
        },
        {
          "data": "${getSampleSetsResult}",
          "property": "Sample_Id",
          "required": true,
          "textProperty": "FilePath",
          "title": "Sample Set",
          "type": "lookup",
          "valueProperty": "Id",
          "placeholder": "Choose SampleSet"
        }
      ],
      "row": 0,
      "section": null,
      "name": "form0",
      "type": "form"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getIrisSetById",
        "parameters": [
          {
            "name": "Id",
            "value": "${parameters.Id}"
          }
        ],
        "then": [
          {
            "name": "irisset",
            "type": "set",
            "value": "${result}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "biometric characteristics model.getSampleSets",
        "parameters": [],
        "then": [
          {
            "name": "getSampleSetsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Iris Set"
}