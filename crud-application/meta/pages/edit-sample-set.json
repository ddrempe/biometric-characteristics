{
  "components": [
    {
      "col": 12,
      "data": "${sampleset}",
      "events": {
        "cancel": [
          {
            "type": "back"
          }
        ],
        "submit": [
          {
            "error": [
              {
                "detail": "Unable to update SampleSet",
                "severity": "error",
                "summary": "Error",
                "type": "notify"
              }
            ],
            "name": "biometric characteristics model.updateSampleSet",
            "parameters": [
              {
                "name": "Id",
                "value": "${parameters.Id}"
              },
              {
                "name": "SampleSet",
                "value": "${event}"
              }
            ],
            "then": [
              {
                "path": "sample-sets",
                "type": "navigate"
              }
            ],
            "type": "invoke"
          }
        ]
      },
      "fields": [
        {
          "property": "DateCreated",
          "required": false,
          "title": "Date Created",
          "type": "date"
        },
        {
          "property": "FileName",
          "required": true,
          "title": "File Name",
          "type": "string"
        },
        {
          "property": "FilePath",
          "required": true,
          "title": "File Path",
          "type": "string"
        },
        {
          "property": "FileType",
          "required": true,
          "title": "File Type",
          "type": "string"
        },
        {
          "property": "FullFilePath",
          "required": true,
          "title": "Full File Path",
          "type": "string"
        },
        {
          "data": "${getDeviceSetsResult}",
          "property": "DeviceId",
          "required": false,
          "textProperty": "Name",
          "title": "Device Set",
          "type": "lookup",
          "valueProperty": "Id",
          "placeholder": "Choose DeviceSet"
        },
        {
          "data": "${getPersonSetsResult}",
          "property": "PersonId",
          "required": true,
          "textProperty": "FirstName",
          "title": "Person Set",
          "type": "lookup",
          "valueProperty": "Id",
          "placeholder": "Choose PersonSet"
        },
        {
          "data": "${getSampleDatasetSetsResult}",
          "property": "SampleDatasetId",
          "required": false,
          "textProperty": "Name",
          "title": "Sample Dataset Set",
          "type": "lookup",
          "valueProperty": "Id",
          "placeholder": "Choose SampleDatasetSet"
        },
        {
          "data": "${getSampleTypeSetsResult}",
          "property": "SampleTypeId",
          "required": true,
          "textProperty": "Name",
          "title": "Sample Type Set",
          "type": "lookup",
          "valueProperty": "Id",
          "placeholder": "Choose SampleTypeSet"
        }
      ],
      "row": 0,
      "section": null,
      "name": "form0",
      "type": "form"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getSampleSetById",
        "parameters": [
          {
            "name": "Id",
            "value": "${parameters.Id}"
          }
        ],
        "then": [
          {
            "name": "sampleset",
            "type": "set",
            "value": "${result}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "biometric characteristics model.getDeviceSets",
        "parameters": [],
        "then": [
          {
            "name": "getDeviceSetsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "biometric characteristics model.getPersonSets",
        "parameters": [],
        "then": [
          {
            "name": "getPersonSetsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "biometric characteristics model.getSampleDatasetSets",
        "parameters": [],
        "then": [
          {
            "name": "getSampleDatasetSetsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "biometric characteristics model.getSampleTypeSets",
        "parameters": [],
        "then": [
          {
            "name": "getSampleTypeSetsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Sample Set",
  "access": ""
}