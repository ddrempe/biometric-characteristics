{
  "components": [
    {
      "name": "grid0",
      "allowDelete": true,
      "allowAdd": true,
      "allowFiltering": true,
      "allowMultiSorting": false,
      "allowPaging": true,
      "allowSorting": true,
      "col": 12,
      "count": "${getKeystrokeSetsCount}",
      "data": "${getKeystrokeSetsResult}",
      "events": {
        "loadData": [
          {
            "name": "biometric characteristics model.getKeystrokeSets",
            "parameters": [
              {
                "name": "$filter",
                "value": "${event.filter}"
              },
              {
                "name": "$top",
                "value": "this.grid0.allowPaging ? event.top : null"
              },
              {
                "name": "$skip",
                "value": "this.grid0.allowPaging ? event.skip : null"
              },
              {
                "name": "$orderby",
                "value": "${event.orderby}"
              },
              {
                "name": "$count",
                "value": "this.grid0.allowPaging"
              },
              {
                "name": "$expand",
                "value": "KeyboardSet,KeystrokeDatasetSet,PersonSet"
              }
            ],
            "then": [
              {
                "name": "getKeystrokeSetsResult",
                "type": "set",
                "value": "${result.value}"
              },
              {
                "name": "getKeystrokeSetsCount",
                "type": "set",
                "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
              }
            ],
            "type": "invoke"
          }
        ],
        "delete": [
          {
            "error": [
              {
                "detail": "Unable to delete KeystrokeSet",
                "severity": "error",
                "summary": "Error",
                "type": "notify"
              }
            ],
            "name": "biometric characteristics model.deleteKeystrokeSet",
            "parameters": [
              {
                "name": "Id",
                "value": "${event.Id}"
              }
            ],
            "then": [
              {
                "detail": "KeystrokeSet deleted!",
                "severity": "success",
                "summary": "Success",
                "type": "notify"
              }
            ],
            "type": "invoke"
          }
        ],
        "add": [
          {
            "path": "add-keystroke-set",
            "type": "popup"
          }
        ],
        "rowSelect": [
          {
            "parameters": [
              {
                "name": "Id",
                "value": "${event.Id}"
              }
            ],
            "path": "edit-keystroke-set",
            "type": "popup"
          }
        ]
      },
      "row": 0,
      "section": null,
      "title": "",
      "columns": [
        {
          "property": "Id",
          "title": "Id",
          "type": "integer"
        },
        {
          "property": "KeyboardId",
          "template": "${data.KeyboardSet?.Name}",
          "title": "Keyboard Set",
          "type": "integer"
        },
        {
          "property": "KeystrokeDatasetId",
          "template": "${data.KeystrokeDatasetSet?.Name}",
          "title": "Keystroke Dataset Set",
          "type": "integer"
        },
        {
          "property": "Password",
          "title": "Password",
          "type": "string"
        },
        {
          "property": "PersonId",
          "template": "${data.PersonSet?.FirstName}",
          "title": "Person Set",
          "type": "integer"
        },
        {
          "property": "PpTime",
          "title": "Pp Time",
          "type": "string"
        },
        {
          "property": "PrTime",
          "title": "Pr Time",
          "type": "string"
        },
        {
          "property": "RawPress",
          "title": "Raw Press",
          "type": "string"
        },
        {
          "property": "RawRelease",
          "title": "Raw Release",
          "type": "string"
        },
        {
          "property": "RpTime",
          "title": "Rp Time",
          "type": "string"
        },
        {
          "property": "RrTime",
          "title": "Rr Time",
          "type": "string"
        },
        {
          "property": "Success",
          "title": "Success",
          "type": "boolean"
        },
        {
          "property": "TimeToType",
          "title": "Time To Type",
          "type": "string"
        },
        {
          "property": "Vector",
          "title": "Vector",
          "type": "string"
        }
      ],
      "type": "grid"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getKeystrokeSets",
        "parameters": [
          {
            "name": "$top",
            "value": "this.grid0.allowPaging ? this.grid0.pageSize : null"
          },
          {
            "name": "$skip",
            "value": "this.grid0.allowPaging ? 0 : null"
          },
          {
            "name": "$count",
            "value": "this.grid0.allowPaging"
          },
          {
            "name": "$expand",
            "value": "KeyboardSet,KeystrokeDatasetSet,PersonSet"
          }
        ],
        "then": [
          {
            "name": "getKeystrokeSetsResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getKeystrokeSetsCount",
            "type": "set",
            "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Keystroke",
  "access": ""
}