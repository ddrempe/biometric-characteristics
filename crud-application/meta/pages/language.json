{
  "access": "",
  "components": [
    {
      "allowAdd": true,
      "allowDelete": true,
      "allowFiltering": true,
      "allowMultiSorting": false,
      "allowPaging": true,
      "allowSorting": true,
      "col": 12,
      "columns": [
        {
          "property": "Id",
          "title": "Id",
          "type": "integer"
        },
        {
          "property": "Name",
          "title": "Name",
          "type": "string"
        },
        {
          "property": "Code",
          "title": "Code",
          "type": "string"
        }
      ],
      "count": "${getLanguageSetsCount}",
      "data": "${getLanguageSetsResult}",
      "events": {
        "add": [
          {
            "path": "add-language-set",
            "type": "popup"
          }
        ],
        "delete": [
          {
            "error": [
              {
                "detail": "Unable to delete LanguageSet",
                "severity": "error",
                "summary": "Error",
                "type": "notify"
              }
            ],
            "name": "biometric characteristics model.deleteLanguageSet",
            "parameters": [
              {
                "name": "Id",
                "value": "${event.Id}"
              }
            ],
            "then": [
              {
                "detail": "LanguageSet deleted!",
                "severity": "success",
                "summary": "Success",
                "type": "notify"
              }
            ],
            "type": "invoke"
          }
        ],
        "loadData": [
          {
            "name": "biometric characteristics model.getLanguageSets",
            "parameters": [
              {
                "name": "$filter",
                "value": "${event.filter}"
              },
              {
                "name": "$top",
                "value": "this.grid0.allowPaging ? event.top : null"
              },
              {
                "name": "$skip",
                "value": "this.grid0.allowPaging ? event.skip : null"
              },
              {
                "name": "$orderby",
                "value": "${event.orderby}"
              },
              {
                "name": "$count",
                "value": "this.grid0.allowPaging"
              },
              {
                "name": "$expand",
                "value": ""
              }
            ],
            "then": [
              {
                "name": "getLanguageSetsResult",
                "type": "set",
                "value": "${result.value}"
              },
              {
                "name": "getLanguageSetsCount",
                "type": "set",
                "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
              }
            ],
            "type": "invoke"
          }
        ],
        "rowSelect": [
          {
            "parameters": [
              {
                "name": "Id",
                "value": "${event.Id}"
              }
            ],
            "path": "edit-language-set",
            "type": "popup"
          }
        ]
      },
      "name": "grid0",
      "row": 0,
      "section": null,
      "title": "",
      "type": "grid"
    }
  ],
  "events": {
    "load": [
      {
        "name": "biometric characteristics model.getLanguageSets",
        "parameters": [
          {
            "name": "$top",
            "value": "this.grid0.allowPaging ? this.grid0.pageSize : null"
          },
          {
            "name": "$skip",
            "value": "this.grid0.allowPaging ? 0 : null"
          },
          {
            "name": "$count",
            "value": "this.grid0.allowPaging"
          }
        ],
        "then": [
          {
            "name": "getLanguageSetsResult",
            "type": "set",
            "value": "${result.value}"
          },
          {
            "name": "getLanguageSetsCount",
            "type": "set",
            "value": "this.grid0.allowPaging ? result['@odata.count'] : ${result.value.length}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Language"
}